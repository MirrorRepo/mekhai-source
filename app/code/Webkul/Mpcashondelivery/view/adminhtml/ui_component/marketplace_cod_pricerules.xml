                        <item name="provider" xsi:type="string">marketplace_cod_pricerules.marketplace_cod_pricerules.listing_top.bookmarks</item>
                        <item name="namespace" xsi:type="string">current.search</item>
                    </item>
                </item>
            </argument>
        </filterSearch>
        <filters name="listing_filters">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="columnsProvider" xsi:type="string">marketplace_cod_pricerules.marketplace_cod_pricerules.marketplace_mpcashondelivery_columns</item>
                    <item name="storageConfig" xsi:type="array">
