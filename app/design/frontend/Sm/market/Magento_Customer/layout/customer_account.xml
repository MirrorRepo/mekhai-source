<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
    </head>
    <body>
        
        <referenceContainer name="sidebar.main">
            <block class="Magento\Framework\View\Element\Template" name="customer_account_navigation_block" template="Magento_Theme::html/collapsible.phtml" before="-">
                <arguments>
                    <argument name="block_title" translate="true" xsi:type="string">Account Dashboard</argument>
                    <argument name="block_css" xsi:type="string">block-collapsible-nav</argument>
                </arguments>
                <block class="Magento\Framework\View\Element\Html\Links" name="customer_account_navigation" before="-">
                    <arguments>
                        <argument name="css_class" xsi:type="string">nav items</argument>
                    </arguments>
                    <block class="Webkul\Marketplace\Block\View\Html\SwitchLink" name="marketplace-seller-dashboard-link" before="-">
                        <arguments>
                            <argument name="label" xsi:type="string" translate="true">Seller Dashboard</argument>
                            <argument name="path" xsi:type="string">marketplace\account\dashboard</argument>
                        </arguments>
                    </block>
                    <block class="Magento\Framework\View\Element\Html\Link\Current" name="customer-account-navigation-account-link" after="marketplace-seller-dashboard-link">
                        <arguments>
                            <argument name="label" xsi:type="string">Account Dashboard</argument>
                            <argument name="path" xsi:type="string">customer/account</argument>
                        </arguments>
                    </block>
                    <block class="Magento\Framework\View\Element\Html\Link\Current" name="customer-account-navigation-account-edit-link" after="customer-account-navigation-account-link">
                        <arguments>
                            <argument name="label" xsi:type="string">Account Information</argument>
                            <argument name="path" xsi:type="string">customer/account/edit</argument>
                        </arguments>
                    </block>
                    <block class="Magento\Framework\View\Element\Html\Link\Current" name="customer-account-navigation-address-link" after="customer-account-navigation-account-edit-link">
                        <arguments>
                            <argument name="label" xsi:type="string">Address Book</argument>
                            <argument name="path" xsi:type="string">customer/address</argument>
                        </arguments>
                    </block>
                </block>
            </block>
        </referenceContainer>
        
        <move element="page.main.title" destination="content.top" before="-"/>
        <!-- Product review link -->
        <referenceBlock name="customer-account-navigation-my-credit-cards-link" remove="true"/>
        <!-- Subscription link -->
        <referenceBlock name="customer-account-navigation-newsletter-subscriptions-link" remove="true"/>
    </body>
</page>
